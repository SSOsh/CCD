<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4AFFFFFF"
    tools:context=".forumEnroll">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="168dp"
        android:background="#F6CED8"
        android:text="forum title"
        android:textAllCaps="true"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/fEnrollBtn"
        android:layout_width="74dp"
        android:layout_height="35dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="616dp"
        android:background="#CB080808"
        android:text="등록"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/fEnrollCancleBtn"
        android:layout_width="74dp"
        android:layout_height="35dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="616dp"
        android:background="#CB080808"
        android:text="취소"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/fEnrollBtn"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="379dp"
        android:layout_height="353dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="16dp"
        android:background="#F8E0F1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="FORUM CONTAIN"
                android:background="#F6CED8"
                android:textSize="18sp" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>